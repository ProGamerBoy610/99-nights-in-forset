-- Executor Script: Teleport to Log in items folder
local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local root = char:WaitForChild("HumanoidRootPart")

-- Direct teleport to Log part
local items = workspace:FindFirstChild("Items")
if items then
    local log = items:FindFirstChild("Log")
    if log and log:IsA("BasePart") then
        root.CFrame = log.CFrame + Vector3.new(0, 3, 0)
        print("✅ Teleported to Log!")
    else
        print("❌ Log part not found!")
    end
else
    print("❌ Items folder not found!")
end
